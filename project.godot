config_version=5

[application]

config/name="Melt Them All"
run/main_scene="res://MainMenu.tscn"
config/features=PackedStringArray("4.2")
boot_splash/bg_color=Color(0.223529, 0.109804, 0.192157, 1)
boot_splash/image="res://splash.png"
config/icon="res://icon.png"

[autoload]

GlobalSteam="*res://Autoload/steam.gd"
Events="*res://Autoload/Events.gd"
Library="*res://Autoload/Library.gd"
RecolorTable="*res://Autoload/RecolorTable.gd"
SoundManager="*res://Sound/SoundManager.tscn"
DamageNumbers="*res://Autoload/DamageNumbers.gd"
PlanetData="*res://Autoload/PlanetData.gd"
